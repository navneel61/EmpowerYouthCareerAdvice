<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/blog_graph"
    app:startDestination="@id/blogHomeFragment">

    <fragment
        android:id="@+id/blogHomeFragment"
        android:name="com.example.empoweryouthcareeradvice.BlogHomeFragment"
        android:label="fragment_blog_home"
        tools:layout="@layout/fragment_blog_home" >
        <action
            android:id="@+id/action_blogHomeFragment_to_blogSummaryFragment"
            app:destination="@id/blogSummaryFragment" />
    </fragment>
    <fragment
        android:id="@+id/blogSummaryFragment"
        android:name="com.example.empoweryouthcareeradvice.BlogSummaryFragment"
        android:label="fragment_blog_summary"
        tools:layout="@layout/fragment_blog_summary" />
</navigation>